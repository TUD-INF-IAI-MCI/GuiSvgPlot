<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<AnchorPane prefHeight="700.0" prefWidth="1000.0" stylesheets="/stylesheet/guiSvgPlott.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.RootFrameController">
    <BorderPane fx:id="borderPane_Content" layoutX="311.0" layoutY="100.0" prefHeight="700.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <top>
            <MenuBar BorderPane.alignment="CENTER">
                <menus>
                    <Menu mnemonicParsing="false" text="%file">
                        <items>
                            <MenuItem fx:id="menuItem_ExitProgram" mnemonicParsing="false" onAction="#closeButtonAction" text="%close" />
                        </items>
                    </Menu>
                    <Menu mnemonicParsing="false" text="%edit">
                        <items>
                            <MenuItem mnemonicParsing="false" text="%delete" />
                        </items>
                    </Menu>
               <Menu mnemonicParsing="false" text="%presets">
                  <items>
                     <MenuItem fx:id="menuItem_Preset_default" mnemonicParsing="false" onAction="#startPreset" text="%default" />
                     <MenuItem fx:id="menuItem_Preset_1" mnemonicParsing="false" onAction="#startPreset" text="%preset_1" />
                     <MenuItem fx:id="menuItem_Preset_2" mnemonicParsing="false" onAction="#startPreset" text="%preset_2" />
                     <MenuItem fx:id="menuItem_Preset_3" mnemonicParsing="false" onAction="#startPreset" text="%preset_3" />
                     <MenuItem fx:id="menuItem_Preset_Editor" mnemonicParsing="false" onAction="#startPresetEditor" text="%set_preset" />
                  </items>
               </Menu>
                    <Menu mnemonicParsing="false" text="%help">
                        <items>
                            <MenuItem mnemonicParsing="false" text="%about" />
                        </items>
                    </Menu>
                </menus>
            </MenuBar>
        </top>
      <center>
         <ButtonBar buttonMinWidth="350.0" maxWidth="-Infinity" prefHeight="675.0" BorderPane.alignment="CENTER">
           <buttons>
                     <Button fx:id="button_StartFunction" accessibleText="Funktion erstellen" mnemonicParsing="false" prefHeight="103.0" text="%function_create" ButtonBar.buttonData="LEFT" />
                     <Button fx:id="button_StartDiagram" accessibleText="Diagramm erstellen" contentDisplay="CENTER" mnemonicParsing="false" prefHeight="103.0" text="%diagram_create" ButtonBar.buttonData="RIGHT" />
           </buttons>
            <BorderPane.margin>
               <Insets />
            </BorderPane.margin>
         </ButtonBar>
      </center>
    </BorderPane>
</AnchorPane>
