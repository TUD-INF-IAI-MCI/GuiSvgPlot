<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<?import javafx.scene.control.Tooltip?>
<AnchorPane maxWidth="1.7976931348623157E308" prefHeight="400.0" prefWidth="1000.0"
            xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="application.controller.wizard.WizardController">
    <BorderPane fx:id="borderPane_Wizard" layoutX="200.0" layoutY="100.0" prefHeight="400.0"
                AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                AnchorPane.topAnchor="0.0">
        <bottom>
            <ButtonBar maxWidth="1.7976931348623157E308" prefHeight="40.0" BorderPane.alignment="CENTER">
                <buttons>
                    <Button fx:id="button_Back" accessibleText="Vorheriger Schritt" mnemonicParsing="false"
                            text="%button_back"
                            ButtonBar.buttonData="RIGHT">
                        <tooltip>
                            <Tooltip text="%button_back"/>
                        </tooltip>
                    </Button>
                    <Button fx:id="button_Next" accessibleText="NÃ¤chster Scritt" mnemonicParsing="false"
                            text="%button_next"
                            ButtonBar.buttonData="RIGHT">
                        <tooltip>
                            <Tooltip text="%button_next"/>
                        </tooltip>
                    </Button>
                    <Button fx:id="button_Cancel" accessibleText="Abbrechen" alignment="TOP_LEFT" cancelButton="true"
                            mnemonicParsing="false" text="%button_cancel" ButtonBar.buttonData="LEFT">
                        <tooltip>
                            <Tooltip text="%button_cancel"/>
                        </tooltip>
                    </Button>
                </buttons>
                <padding>
                    <Insets left="10.0" right="10.0"/>
                </padding>
            </ButtonBar>
        </bottom>
        <center>

        </center>
        <top>
            <Label fx:id="label_Headline" alignment="CENTER" contentDisplay="CENTER" minWidth="200.0" prefHeight="48.0"
                   text="Label" textAlignment="CENTER" BorderPane.alignment="CENTER"/>
        </top>
    </BorderPane>
</AnchorPane>
